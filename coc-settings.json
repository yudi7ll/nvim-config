{
  "html.format.wrapAttributes": "auto",
  "html.format.indentInnerHtml": true,
  "html.format.wrapLineLength": 0,
  "html.filetypes": [
    "blade",
    "html",
    "handlebars",
    "htmldjango",
    "blade",
    "ejs"
  ],
  "bladeFormatter.format.wrapLineLength": 1000,
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "emmet.includeLanguages": {
    "blade": "html",
    "ejs": "html",
    "jsx": "javascriptreact",
    "javascript": "javascriptreact"
  },
  "diagnostic.errorSign": " ",
  "diagnostic.warningSign": " ",
  "diagnostic.hintSign": " ",
  "diagnostic.infoSign": " ",
  "coc.preferences.enableMessageDialog": true,
  "coc.preferences.rootPatterns": [
    ".git",
    ".hg",
    ".projections.json",
    ".env"
  ],
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "javascriptreact",
    "jsonc",
    "php",
    "css",
    "sh",
    "python",
    "markdown",
    "json",
    "html"
  ],
  "Lua.diagnostics.globals": [
    "vim"
  ],
  "highlight.colorNames.enable": false,
  "suggest.timeout": 5000,
  "suggest.acceptSuggestionOnCommitCharacter": true,
  "suggest.completionItemKindLabels": {
    "text": "",
    "method": "ƒ",
    "function": "",
    "constructor": "",
    "variable": "",
    "class": "",
    "interface": "ﰮ",
    "module": "",
    "property": "",
    "unit": "",
    "value": "",
    "enum": "了",
    "keyword": "",
    "snippet": "﬌",
    "color": "",
    "file": "",
    "folder": "",
    "constant": "",
    "struct": "",
    "operator": "\u03a8",
    "reference": "\ufa46",
    "enumMember": "\uf02b",
    "field": "\uf9be",
    "event": "\ufacd",
    "typeParameter": "\uf728",
    "default": "\uf29c"
  },
  "javascript.format.insertSpaceBeforeFunctionParenthesis": true,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingEmptyBraces": true,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingJsxExpressionBraces": true,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
  "tsserver.typingsCacheLocation": "~/.cache",
  "tsserver.implicitProjectConfig.experimentalDecorators": true,
  "eslint.autoFixOnSave": true,
  "eslint.codeActionsOnSave.mode": "all",
  "eslint.format.enable": true,
  "tailwindCSS.emmetCompletions": true,
  "suggest.noselect": false,
  "suggest.languageSourcePriority": 1,
  "suggest.triggerCompletionWait": 0,
  "list.previewSplitRight": true
}
