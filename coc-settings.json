{
  "html.format.wrapAttributes": "auto",
  "html.format.indentInnerHtml": true,
  "html.format.wrapLineLength": 0,
  "html.filetypes": [
    "blade", "html", "handlebars",
    "htmldjango",
    "blade",
    "ejs"
  ],
  "blade.bladeFormatter.optWrapAttributes": "force-expand-multiline",
  "emmet.includeLanguages": {
    "blade": "html",
    "ejs": "html",
    "jsx": "javascriptreact",
    "javascript": "javascriptreact"
  },
  "emmet.priority": 20,
  "coc.source.file.priority": 10,
  "coc.source.around.priority": 15,
  "coc.source.buffer.priority": 15,
  "diagnostic.errorSign": " ",
  "diagnostic.warningSign": " ",
  "diagnostic.hintSign": " ",
  "diagnostic.infoSign": " ",
  "diagnostic.separateRelatedInformationAsDiagnostics": true,
  "diagnostic.checkCurrentLine": true,
  "coc.preferences.rootPatterns": [
    ".git",
    ".hg",
    ".projections.json",
    ".env"
  ],
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "php",
    "css",
    "sh",
    "python",
    "markdown",
    "html",
    "lua"
  ],
  "coc.preferences.jumpCommand": "drop",
  "coc.preferences.extensionUpdateCheck": "never",
  "Lua.diagnostics.globals": [
    "vim"
  ],
  "highlight.colorNames.enable": false,
  "suggest.echodocSupport": true,
  "suggest.timeout": 15000,
  "suggest.maxCompleteItemCount": 30,
  "suggest.acceptSuggestionOnCommitCharacter": true,
  "suggest.completionItemKindLabels": {
    "text": "",
    "method": "ƒ",
    "function": "",
    "constructor": "",
    "variable": "",
    "class": "",
    "interface": "ﰮ",
    "module": "",
    "property": "",
    "unit": "",
    "value": "",
    "enum": "了",
    "keyword": "",
    "snippet": "﬌",
    "color": "",
    "file": "",
    "folder": "",
    "constant": "",
    "struct": "",
    "operator": "\u03a8",
    "reference": "\ufa46",
    "enumMember": "\uf02b",
    "field": "\uf9be",
    "event": "\ufacd",
    "typeParameter": "\uf728",
    "default": "\uf29c"
  },
  // "javascript.format.insertSpaceBeforeFunctionParenthesis": true,
  // "javascript.format.insertSpaceAfterOpeningAndBeforeClosingEmptyBraces": true,
  // "javascript.format.insertSpaceAfterOpeningAndBeforeClosingJsxExpressionBraces": true,
  // "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,

  // "typescript.format.insertSpaceBeforeFunctionParenthesis": true,
  // "typescript.format.insertSpaceAfterOpeningAndBeforeClosingEmptyBraces": true,
  // "typescript.format.insertSpaceAfterOpeningAndBeforeClosingJsxExpressionBraces": true,
  // "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,

  "tsserver.implicitProjectConfig.checkJs": false,
  "tsserver.npm": "$HOME/.local/bin/npm",
  "typescript.preferences.importModuleSpecifier": "project-relative",
  "javascript.preferences.importModuleSpecifier": "project-relative",
  "eslint.autoFixOnSave": true,
  "eslint.onIgnoredFiles": "off",
  "eslint.quiet": true,
  "eslint.validate": [
    "vue",
    "html",
    "css",
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact"
  ],
  "tailwindCSS.emmetCompletions": true,
  "suggest.disableMenu": true,
  "suggest.noselect": false,
  "suggest.languageSourcePriority": -1,
  "suggest.triggerCompletionWait": 30,
  "suggest.defaultSortMethod": "none",
  "suggest.enablePreview": true,
  "list.previewSplitRight": true,
  "lightbulb.enableSign": true,
  "lightbulb.enableVirtualText": false
}
